a=$(($RANDOM % 1000))
b=$(($RANDOM % 1000))
c=$(($RANDOM % 1000))
d=$(($RANDOM % 1000))
e=$(($RANDOM % 1000))
if [ $a -gt  $b && -gt $c && -gt $d && -gt $e ]
then 
echo "max value is $a"
elif [ $b -gt $a && -gt $c && -gt $d && -gt $e ]
then 
echo "max value is $b"
elif [ $c -gt $a && -gt $b && -gt $d && -gt $e ]
then
echo "max value is $c"
elif [ $d -gt $a && -gt $c && -gt $b && -gt $e ]
then
"max value is $d"
else
echo "max value is $e" 
fi
